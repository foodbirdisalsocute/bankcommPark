    top: 80rpx;
}
.plate-view button{
    margin-top: 10px;
    height: 20px;
    background-color: white;
    color: #6892FD;
    font-size: 12px;
    border-radius: 43rpx;
    display: flex;
    align-items: center;
    justify-content:center;

}

