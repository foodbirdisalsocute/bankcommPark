    height: 220rpx;
}

.plate-text {
    color: white;
    font-size: 40rpx;
    font-weight: 550;
    position: absolute;
    left: 90rpx;
    top: 90rpx;
}
.plate-view {
    color: white;
    font-size: 40rpx;
